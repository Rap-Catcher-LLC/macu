clear ; close all; clc
data = load('/home/pratikku/test/macu/metrics/d9_dm_vs_204_11_56_45.current_connection');

%Read data
X = data(:,1);
y = data(:,2);

%plot data
figure;
plot(X, y,'rx','MarkerSize',10);
xlabel('timestamp')
ylabel('value')

%Prepare X
m = size(X,1);
degree = 7;
X = mapFeature(X, degree);
%Feature Normalize

X = [ones(m,1) X];





